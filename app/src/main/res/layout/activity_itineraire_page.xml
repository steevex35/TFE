    <fragment
        android:id="@+id/mapView"
        android:name="com.google.android.gms.maps.MapFragment"
        android:layout_marginTop="57dp"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>

</RelativeLayout>
