    android:layout_height="fill_parent"
    tools:context="com.obisteeves.meetuworld.PageAndroid.ItinerairePage">

    <include
        android:id="@+id/tool_bar"
        layout="@layout/tool_bar"
        android:layout_height="wrap_content"
